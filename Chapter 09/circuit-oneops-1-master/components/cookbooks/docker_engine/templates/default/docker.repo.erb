[dockerrepo]
name=Docker Repository
<%
  repo = get_docker_repo
  is_official_repo = repo.include?('https://yum.dockerproject.org')
%>
baseurl=<%= repo %>
enabled=1
<%
  if is_official_repo
%>
gpgcheck=1
gpgkey=https://yum.dockerproject.org/gpg
<% else %>
gpgcheck=0
<% end %>
